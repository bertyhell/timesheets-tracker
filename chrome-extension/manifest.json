{
  "name": "Timesheet tracker",
  "version": "0.0.1",
  "manifest_version": 3,
  "description": "Tracks which sites you visit and for how long and sends that info to the timesheet tracker application",
  "homepage_url": "https://github.com/bertyhell/timesheet-tracker",
  "icons": {
    "16": "icons/icon16.png",
    "19": "icons/icon19.png",
    "38": "icons/icon38.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "options_page": "src/options/index.html",
  "action": {
    "default_title": "Timesheet tracker",
    "default_icon": {
      "16": "icons/icon16.png",
      "19": "icons/icon19.png",
      "38": "icons/icon38.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "background": {
    "service_worker": "src/background/background.js"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "scripting",
    "storage"
  ]
}
