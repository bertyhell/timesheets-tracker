{
  "name": "vite-reactts-eslint-prettier",
  "version": "0.0.0",
  "scripts": {
    "dev:client": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "lint:fix": "eslint ./src --ext .jsx,.js,.ts,.tsx --quiet --fix --ignore-path ./.gitignore",
    "lint:format": "prettier  --loglevel warn --write \"./**/*.{js,jsx,ts,tsx,css,md,json}\" ",
    "lint": "yarn lint:format && yarn lint:fix ",
    "type-check": "tsc",
    "generate-api-helpers": "openapi-rq -c fetch -i http://localhost:55577/docs-yaml -o ./src/generated/api && npx tsx scripts/ignore-types-in-generated-files.ts",
    "bump": "npx bump-package-versions --part=patch --strategy=highest ./package.json ../package.json ../api/package.json"
  },
  "dependencies": {
    "@adobe/react-spectrum": "^3.34.1",
    "@tanstack/react-query": "4.36.1",
    "@tippyjs/react": "4.2.6",
    "blend-promise-utils": "^1.29.0",
    "copy-to-clipboard": "3.3.3",
    "date-fns": "2.30.0",
    "jotai": "^2.6.2",
    "lodash-es": "4.17.21",
    "react": "18.2.0",
    "react-colorful": "^5.6.1",
    "react-dom": "18.2.0",
    "react-responsive-modal": "6.4.2",
    "react-router-dom": "6.20.0",
    "react-select": "5.7.4",
    "react-toastify": "^10.0.4",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@7nohe/openapi-react-query-codegen": "0.5.0",
    "@types/lodash-es": "4.17.8",
    "@types/react": "18.2.28",
    "@types/react-dom": "18.2.13",
    "@typescript-eslint/eslint-plugin": "5.10.2",
    "@typescript-eslint/parser": "5.10.2",
    "@vitejs/plugin-react": "1.3.2",
    "autoprefixer": "10.4.15",
    "eslint": "8.8.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.28.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "postcss": "8.4.29",
    "pre-commit": "1.2.2",
    "prettier": "2.5.1",
    "sass": "1.64.2",
    "tailwindcss": "3.3.3",
    "typescript": "5.2.2",
    "vite": "4.4.11"
  },
  "pre-commit": "lint",
  "license": "MIT"
}
